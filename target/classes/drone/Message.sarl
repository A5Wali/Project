 import drone.TypeMessage
class Message <T> {
	
	var type : drone.TypeMessage
	var message : T
	
	new(t : TypeMessage, m : T){
		type = t
		message = m
	}
	
	def getType() : TypeMessage{
		return type
	}
	
	def setType(type : TypeMessage){
		this.type = type;
	}
	
	def getMessage() : T{
		return message;
	}
	def setMessage(message : T){
		this.message = message
	}
}
