package drone

import java.io.Serializable
import javax.vecmath.Point3f

/** 
 * @author Alexandre
 * 
 */
class Cube extends EnvObj implements Serializable {
	private var width : float
	
	new(){
		super()
		this.width = 0.0f
	}
	
	new(pos : Point3f, w : float){
		super(pos)
		this.width = w
	}
	
	def setWidth(w : float){
		this.width = w
	}
	
	def getWidth() : float {
		this.width
	}
}

